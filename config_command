#!/bin/sh
# Generated by configure.
# Run this file to recreate the current configuration.
# Compiler output produced by configure, useful for debugging
# configure, is in config.log if it exists.
unset AR
unset AS
unset CC
unset CPP
unset CXX
unset INSTALL
unset LD
unset LD_LIBRARY_PATH
unset LIBTOOL
unset MAKE
unset NM
unset OBJCOPY
PATH='/home/martin/.cargo/bin:/home/martin/bin:/home/martin/.local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games:/snap/bin'
export PATH
unset PKG_CONFIG
unset PKG_CONFIG_LIBDIR
unset PKG_CONFIG_PATH
unset PYTHON
unset SDL2_CONFIG
unset SMBD
unset STRIP
unset WINDRES
exec '../configure' '--target-list=xtensa-softmmu' '--enable-gcrypt' '--disable-user' '--disable-capstone' '--disable-vnc' '--enable-vte' '--disable-fdt' '--disable-iconv' '--disable-curses' '--disable-sdl' '--disable-parallels' '--disable-bochs' '--disable-cloop' '--disable-dmg' '--disable-vdi' '--disable-vvfat' '--disable-qed' '--disable-debug-info' '--disable-xkbcommon' '--disable-libudev' '--extra-ldflags=-Wl,-rpath=$ORIGIN' '--disable-opengl' '--disable-vhost-user' '--disable-vhost-kernel' "$@"
